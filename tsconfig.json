{
	"compilerOptions": {
		"module": "es6",
		"target": "es6",
		"lib": ["dom", "es2019"],
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": false,
		"sourceMap": false,
		"allowJs": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"moduleResolution": "node"
	},
	"exclude": ["dist", "build", "node_modules"]
}
